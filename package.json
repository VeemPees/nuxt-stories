{
  "name": "nuxt-stories",
  "version": "0.0.7",
  "description": "Nuxt stories module -- Painless storybooking for Nuxt",
  "author": "Richard Schloss",
  "main": "modules/stories.module.js",
  "license": "MIT",
  "keywords": [
    "nuxt",
    "storybook",
    "stories",
    "painless"
  ],
  "contributors": [
    {
      "name": "Richard Schloss"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/richardeschloss/nuxt-stories",
  "scripts": {
    "dev": "nuxt",
    "dev:server": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:stories": "eslint --ext .js,.vue .stories/**/* --fix --no-ignore",
    "lint:modules": "eslint --ext .js,.vue modules/**/* --fix --no-ignore",
    "test": "ava",
    "test:unit": "cross-env TEST=unit ava --config unit.config.js",
    "test:unit:watch": "cross-env TEST=unit ava --config unit.config.js --watch",
    "test:e2e": "cross-env TEST=e2e ava --config e2e.config.js",
    "postinstall": "node postinstall.js"
  },
  "files": [
    ".stories",
    "modules/stories.module.js",
    "postinstall.js"
  ],
  "dependencies": {
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0",
    "gently-copy": "^3.2.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "browser-env": "^3.2.5",
    "consola": "^2.10.1",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "express": "^4.16.4",
    "nodemon": "^1.18.9",
    "nuxt": "^2.0.0",
    "prettier": "^1.16.4",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^2.0.0",
    "glob": "^7.1.5",
    "pify": "^4.0.1",
    "@nuxt/utils": "^2.10.2"
  }
}
